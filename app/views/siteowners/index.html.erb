<p id="notice"><%= notice %></p>

<h1>Siteowners</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @siteowners.each do |siteowner| %>
      <tr>
        <td><%= link_to 'Show', siteowner %></td>
        <td><%= link_to 'Edit', edit_siteowner_path(siteowner) %></td>
        <td><%= link_to 'Destroy', siteowner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Siteowner', new_siteowner_path %>
