<p id="notice"><%= notice %></p>

<h1>Voodoos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voodoos.each do |voodoo| %>
      <tr>
        <td><%= link_to 'Show', voodoo %></td>
        <td><%= link_to 'Edit', edit_voodoo_path(voodoo) %></td>
        <td><%= link_to 'Destroy', voodoo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voodoo', new_voodoo_path %>
